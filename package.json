{
  "name": "minion-family",
  "private": true,
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start-public": "ng serve --host 0.0.0.0  --disable-host-check",
    "build": "ng build",
    "build:prod": "ng build  --prod",
    "test": "jest",
    "lint": "ng lint",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "e2e": "ng e2e",
    "git": "run-s git:add git:commit git:push",
    "git:commit": "git commit -m \"update dependencies\"",
    "git:add": "git add package.json package-lock.json",
    "git:push": "git push",
    "dependencies:update": "ncu -u -x typescript,ngx-mqtt",
    "dependencies:install": "npm install",
    "update:dependencies and build": "run-s dependencies:update dependencies:install build git"
  },
  "version": "0.0.0",
  "dependencies": {
    "@angular/animations": "11.2.11",
    "@angular/cdk": "11.2.10",
    "@angular/common": "11.2.11",
    "@angular/compiler": "11.2.11",
    "@angular/core": "11.2.11",
    "@angular/forms": "11.2.11",
    "@angular/material": "11.2.10",
    "@angular/platform-browser": "11.2.11",
    "@angular/platform-browser-dynamic": "11.2.11",
    "@angular/router": "11.2.11",
    "@fortawesome/angular-fontawesome": "0.8.2",
    "@fortawesome/fontawesome-svg-core": "1.2.35",
    "@fortawesome/free-brands-svg-icons": "5.15.3",
    "@fortawesome/free-regular-svg-icons": "5.15.3",
    "@fortawesome/free-solid-svg-icons": "5.15.3",
    "cropperjs": "1.5.11",
    "chance": "1.1.7",
    "lodash": "4.17.21",
    "chroma-js": "2.1.1",
    "gsap": "3.6.1",
    "npm-check-updates": "11.5.1",
    "rxjs": "6.6.7",
    "html2canvas": "1.0.0-rc.7",
    "tslib": "2.2.0",
    "uuid": "8.3.2",
    "zone.js": "0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.1102.10",
    "@angular/cli": "11.2.10",
    "@angular/compiler-cli": "11.2.11",
    "@angular/language-service": "11.2.11",
    "@types/chance": "1.1.1",
    "@types/chroma-js": "2.1.3",
    "@types/gsap": "1.20.2",
    "@types/jasmine": "3.6.9",
    "@types/jasminewd2": "2.0.8",
    "@types/node": "14.14.41",
    "@types/uuid": "8.3.0",
    "@types/lodash": "4.14.168",
    "codelyzer": "6.0.1",
    "jasmine-core": "3.7.1",
    "jasmine-spec-reporter": "7.0.0",
    "jest": "26.6.3",
    "npm-run-all": "4.1.5",
    "karma": "6.3.2",
    "karma-chrome-launcher": "3.1.0",
    "karma-coverage-istanbul-reporter": "3.0.3",
    "karma-jasmine": "4.0.1",
    "karma-jasmine-html-reporter": "1.5.4",
    "prettier": "2.2.1",
    "prettier-package-json": "2.1.3",
    "protractor": "7.0.0",
    "ts-jest": "26.5.5",
    "ts-node": "9.1.1",
    "tslint": "6.1.3",
    "typescript": "4.0.6"
  },
  "prettier": {
    "printWidth": 120,
    "singleQuote": true,
    "trailingComma": true,
    "useTabs": false,
    "tabWidth": 2,
    "semi": true,
    "bracketSpacing": true,
    "arrowParens": "always"
  }
